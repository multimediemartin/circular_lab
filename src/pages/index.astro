<style>
  .upcomingEvents {
    background-color: var(--dark);
    padding: 20px 0px;
  }

  .upcomingEvents img {
    border-radius: 20px;
    margin: 20px;
  }
  .heroImg {
    width: 100%;
    height: 40vw;
    object-fit: cover;
  }

  .hero {
    background-color: var(--blue);
    color: white;
    padding: 20px;
  }
  .instagramFeed {
    display: grid;
    grid-template-columns: 1fr 1fr;
  }
  h2 {
